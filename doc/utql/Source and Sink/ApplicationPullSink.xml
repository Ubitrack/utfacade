    <Pattern name="ApplicationPullSinkButton" displayName="Application Pull Sink (Button)">
    	<Description><p xmlns="http://www.w3.org/1999/xhtml">This is a sink component which may be used to interface the dataflow network 
    	to an user application. This sink uses a pull input port and also exhibits a pull interface to 
    	the application.<br/>
    	An application can call the <h:code>get( t )</h:code> method to pull events from the data flow
    	network. For more details, see the Doxygen documentation.
    	</p></Description>
    	
        <Input>
            <Node name="A" displayName="A"/>
            <Node name="B" displayName="B"/>
            <Edge name="Input" source="A" destination="B" displayName="Input Button">
            	<Description><h:p>The input button</h:p></Description>
                <Predicate>type=='Button'&amp;&amp;mode=='pull'</Predicate>
            </Edge>
        </Input>
                
        <DataflowConfiguration>
            <UbitrackLib class="ApplicationPullSinkButton"/>

            <Attribute name="metadataSensorGroup" default="default" displayName="Sensor Group" xsi:type="StringAttributeReferenceType"/>
            <Attribute name="metadataSensorCategory" value="UNDEFINED" xsi:type="EnumAttributeReferenceType"/>
            <Attribute name="metadataSensorName" default="default" displayName="Sensor Name" xsi:type="StringAttributeReferenceType"/>
            <Attribute name="metadataSensorInfo" default="" displayName="Sensor Info" xsi:type="StringAttributeReferenceType"/>
            <Attribute name="metadataDebug" value="FALSE" xsi:type="EnumAttributeReferenceType"/>
            <Attribute name="metadataSynchronization" value="FREE" xsi:type="EnumAttributeReferenceType"/>
            <Attribute name="metadataUsage" value="STREAM" xsi:type="EnumAttributeReferenceType"/>
        </DataflowConfiguration>
    </Pattern>

